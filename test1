<script>
  $(document).ready(function() {

    // Hide the show-less-button initially
    $('.show-less-button').hide();

    // View More button click event
    $('.view-more-button').on('click', function() {
      $('.content-container').addClass("expanded");
      $('.show-less-button').show();
      $(this).hide();
    });

    // Show Less button click event
    $('.show-less-button').on('click', function() {
      $('.content-container').removeClass("expanded");
      $('.view-more-button').show();
      $(this).hide();
    });

    // Toggle Fade Effect
    var button = $('.view-more-button');
    var container = $('.content-container');
    var showLessButton = $('.show-less-button');

    if (button.length > 0 && container.length > 0 && showLessButton.length > 0) {
      button.on('click', function() {
        container.addClass('hide-gradient');
      });

      showLessButton.on('click', function() {
        container.removeClass('hide-gradient');
      });
    }
  });
</script>
</div>
