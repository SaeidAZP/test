<script>
  window.addEventListener('load', function(){
    if(window.location.pathname.includes('/products/')){
      document.addEventListener( 'click', function( event ) {
        if(event.target.closest('[id="add-to-cart"]')){
          gtag('event', 'conversion', {
            'send_to': 'AW-473252048/zGnCCKLi0OoBENCB1eEB',
            'value': parseFloat(document.querySelector('.money').innerText.replace(/[^0-9.]/g,''))
          });
        }
      }, true);
      document.addEventListener( 'click', function( event ) {
        if(event.target.closest('button') && event.target.closest('button').innerText.includes('BUY IT NOW')){
          gtag('event', 'conversion', {
            'send_to': 'AW-473252048/H8jFCKXi0OoBENCB1eEB',
            'value': parseFloat(document.querySelector('.money').innerText.replace(/[^0-9.]/g,''))
          });
        }
      }, true);  
    }
    document.addEventListener( 'click', function( event ) {
      if(event.target.closest('.add-to-cart') && window.location.pathname == '/'){
        gtag('event', 'conversion', {
          'send_to': 'AW-473252048/zGnCCKLi0OoBENCB1eEB',
          'value': parseFloat(event.target.closest('.product-add-cart').parentElement.querySelector('.money').innerText.replace(/[^0-9.]/g,''))
        });
      }
    }); 
  });
</script>
